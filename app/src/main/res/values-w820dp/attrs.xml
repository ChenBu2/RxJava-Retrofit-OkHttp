<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingView">
        <attr name="view_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="RecyclerListView">
        <attr name="orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GifViewTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>

    <style name="Widget.GifMoviewView" parent="@android:style/Widget"></style>
</resources>